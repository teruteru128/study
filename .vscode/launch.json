    "version": "0.2.0",
    "configurations": [
        {
            "name": "prime6 (gdb) 接続",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/src/prime6",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "gdb の再フォーマットを有効にする",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
